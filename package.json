{
  "name": "@shoryabaj/formyx",
  "version": "0.1.0",
  "description": "A form validation Library for React built with TypeScript.",
  "license": "MIT",
  "author": "Shorya Baj",
  "type": "module",
  "main": "./dist/cjs/index.js",
  "module": "./dist/esm/index.esm.js",
  "types": "./dist/index.d.ts",
  "style": "./dist/formyx.css",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bajshorya/Formyx.git"
  },
  "publishConfig": {
    "access": "public"
  },
  "homepage": "https://github.com/bajshorya/Formyx.git",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/esm/index.esm.js"
      },
      "require": {
        "types": "./dist/index.d.ts",
        "default": "./dist/cjs/index.js"
      }
    },
    "./style": "./dist/formyx.css",
    "./style.css": "./dist/formyx.css",
    "./formyx.css": "./dist/formyx.css",
    "./all": "./dist/all.css",
    "./all.css": "./dist/all.css",
    "./dist/*": "./dist/*"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "rollup --config",
    "dev": "rollup --config --watch",
    "prepare": "husky"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^28.0.0",
    "@rollup/plugin-json": "^6.1.0",
    "@rollup/plugin-node-resolve": "^15.1.0",
    "@rollup/plugin-terser": "^0.4.4",
    "@rollup/plugin-typescript": "^11.1.6",
    "@types/react": "^19.2.4",
    "@types/react-dom": "^19.2.3",
    "husky": "^9.1.7",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "rollup": "^4.53.2",
    "rollup-plugin-dts": "^6.2.3",
    "rollup-plugin-postcss": "^4.0.2",
    "tslib": "^2.8.1",
    "typescript": "^5.9.3"
  },
  "peerDependencies": {
    "react": "^19.0.0",
    "react-dom": "^19.0.0"
  }
}
